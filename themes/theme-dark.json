{
	"$schema": "vscode://schemas/color-theme",
	"name": "processing-pama1234",
	"colors": {
		"editor.background": "#1E1E1E",
		"editor.foreground": "#d4d4d4",
		"editorLineNumber.foreground": "#787878",
		"editorLineNumber.activeForeground": "#d6d6d6",
		"editor.selectionBackground": "#745c00",
		"editor.selectionHighlightBackground": "#6b614c",
		"editor.lineHighlightBackground": "#152d30",
	},
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"*.declaration": {
			"fontStyle": "bold"
		},
		"property.declaration": {
			"fontStyle": ""
		},
		"*.static": {
			"fontStyle": "italic"
		},
		"method.static.declaration": {
			"fontStyle": "bold italic"
		},
		"property.readonly": {
			"fontStyle": "bold"
		},
		"property.static.readonly": {
			"fontStyle": "bold italic"
		},
		"property.static": {
			"fontStyle": "italic"
		},
		"property.static.declaration": {
			"fontStyle": "italic"
		},
		"enumMember.readonly": {
			"fontStyle": "bold"
		},
		"enumMember.static.readonly": {
			"fontStyle": "bold italic"
		},
		"interface": {
			"foreground": "#6D5CB4",
			"fontStyle": "bold"
		},
		"enum": {
			"foreground": "#caab96",
			"fontStyle": "bold"
		},
		"typeParameter": {
			"foreground": "#a87554",
			"fontStyle": "bold"
		},
		"macro": {
			"foreground": "#e248d5",
		},
		"module": {
			"foreground": "#f7777b",
			"fontStyle": "bold"
		}
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#a0c291"
			}
		},
		//---
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#6466FF"
			}
		},
		//---
		{
			"scope": [
				"variable",
				"entity.name.variable"
			],
			"settings": {
				"foreground": "#DD4B87"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#BF57CA"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#009dea"
			}
		},
		//---
		{
			"scope": [
				"punctuation",
				"punctuation.bracket",
				// "punctuation.bracket.angle",
				"punctuation.separator.delimiter",
				"storage.type.generic.wildcard"
			],
			"settings": {
				"foreground": "#d4d4d4",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"storage.type.annotation.java",
				"storage.type.function.arrow",
				"punctuation.definition.annotation.java"
			],
			"settings": {
				"foreground": "#dfdfdfC0",
				"fontStyle": ""
			}
		},
		//---
		{
			"scope": "comment.block.javadoc.java",
			"settings": {
				"foreground": "#5F7FDF"
			}
		},
		{
			"scope": [
				"keyword.control",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#e50099",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"keyword",
				"storage.modifier",
				"keyword.operator.expression",
				"keyword.operator.new",
				"keyword.operator.instanceof",
				"keyword.control.new.java",
				"variable.language",
				"support.variable",
				"constant.language",
				"keyword.control.import.python",
				"storage.type",
			],
			"settings": {
				"foreground": "#44cdcd",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "source.groovy.embedded",
			"settings": {
				"foreground": "#44cdcd"
			}
		},
		{
			"scope": [
				"storage.modifier.import.java",
				"storage.modifier.package.java",
				"keyword.operator",
				"keyword.control.ternary.java",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#d4d4d4",
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword.other.documentation.javadoc",
			"settings": {
				"foreground": "#5F7FDF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"meta.class.identifier.java",
				"meta.definition.class.inherited.classes.java",
				"entity.name.tag",
				"entity.name.type.class",
				"storage.type.groovy",
				"storage.type.java",
				"entity.name.type",
				"entity.name.class.message.proto",
				"entity.name.class.declaration",
				"entity.name.class"
			],
			"settings": {
				"foreground": "#FB6104",
			}
		},
		{
			"scope": [
				"punctuation.bracket.angle",
				"punctuation.separator.delimiter",
				"storage.type.generic.wildcard"
			],
			"settings": {
				"foreground": "#d4d4d4",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"storage.type.annotation.java",
				"storage.type.function.arrow",
				"punctuation.definition.annotation.java"
			],
			"settings": {
				"foreground": "#dfdfdfC0",
				"fontStyle": ""
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#E96DB4",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"entity.other.attribute-name",
				"constant.other.key",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#E96DB4"
			}
		},
		{
			"scope": [
				"support.type.property-name",
				"punctuation.support.type.property-name"
			],
			"settings": {
				"foreground": "#E96DB4"
			}
		},
		{
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#d37adb",
				"fontStyle": "bold",
			}
		},
		{
			"scope": "punctuation.section.embedded",
			"settings": {
				"foreground": "#E96DB4",
			}
		},
		{
			"scope": "string.quoted.double.html",
			"settings": {
				"foreground": "#c786ca"
			}
		},
	]
}